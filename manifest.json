{
  "name": "Turkey Dinner",
  "version": "1.0",
  "manifest_version": 2,
  "description": "For Chromium+Webkit developers waiting for the next roll.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "not loaded",
    "default_popup": "popup.html"
  },
  "background": { "scripts": ["jquery-1.8.0.min.js", "linkify.js", "shared.js", "fetch.js"] },
  "content_security_policy": "script-src 'self' https://www.google.com/; object-src 'self'",
  "permissions": [
      "https://www.google.com/jsapi",
      "http://codereview.chromium.org/",
      "http://git.chromium.org/",
      "http://webkit-commit-queue.appspot.com/",
      "https://chromium-build.appspot.com/",
      "https://chromium-status.appspot.com/",
      "https://codereview.chromium.org/"
  ]
}
